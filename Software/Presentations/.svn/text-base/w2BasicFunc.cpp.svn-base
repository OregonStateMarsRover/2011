#include <iostream>

/**
 * squareValue
 *
 * A basic function
 *
 * \param x input to the function
 * \return returns x^2
 */
int squareValue(int x) {
    return x*x;
}

/**
 * Main function
 *
 * Similar to the hello world program
 */
int main(void) {
    //call the function and print the result
    std::cout << squareValue(14) << std::endl;
    return 0;
}

